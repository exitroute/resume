<section class="row" id="about">
  <div class="co1-12 col-md-4 text-md-right">
    <h2>
      About
    </h2>
  </div>
  <div class="col-12 col-md-8">
    <%- site.resume.about.description.html %>

      <dl class="row">
        <% site.resume.about.overview.forEach((entry) => { %>
          <dt class="col-4">
            <%= entry.stage %>
          </dt>
          <dd class="col-8">
            <%= entry.name %>
              <br>
              <%= entry.detail %>
          </dd>
          <% }); %>
      </dl>
  </div>
</section>